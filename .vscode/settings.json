{
    "files.exclude":
    {
        "**/.DS_Store":true,
        "**/.git":true,
        "**/.gitmodules":true,
        "**/*.booproj":true,
        "**/*.pidb":true,
        "**/*.suo":true,
        "**/*.user":true,
        "**/*.userprefs":true,
        "**/*.unityproj":true,
        "**/*.dll":true,
        "**/*.exe":true,
        "**/*.pdf":true,
        "**/*.mid":true,
        "**/*.midi":true,
        "**/*.wav":true,
        "**/*.gif":true,
        "**/*.ico":true,
        "**/*.jpg":true,
        "**/*.jpeg":true,
        "**/*.png":true,
        "**/*.psd":true,
        "**/*.tga":true,
        "**/*.tif":true,
        "**/*.tiff":true,
        "**/*.3ds":true,
        "**/*.3DS":true,
        "**/*.fbx":true,
        "**/*.FBX":true,
        "**/*.lxo":true,
        "**/*.LXO":true,
        "**/*.ma":true,
        "**/*.MA":true,
        "**/*.obj":true,
        "**/*.OBJ":true,
        "**/*.asset":true,
        "**/*.cubemap":true,
        "**/*.flare":true,
        "**/*.mat":true,
        "**/*.meta":true,
        "**/*.prefab":true,
        "**/*.unity":true,
        "build/":true,
        "Build/":true,
        "Library/":true,
        "library/":true,
        "obj/":true,
        "Obj/":true,
        "ProjectSettings/":true,
        "temp/":true,
        "Temp/":true
    },
    "workbench.colorCustomizations": {
        "activityBar.activeBackground": "#164a8e",
        "activityBar.background": "#164a8e",
        "activityBar.foreground": "#e7e7e7",
        "activityBar.inactiveForeground": "#e7e7e799",
        "activityBarBadge.background": "#e13e85",
        "activityBarBadge.foreground": "#e7e7e7",
        "commandCenter.border": "#e7e7e799",
        "sash.hoverBorder": "#164a8e",
        "statusBar.background": "#0f3362",
        "statusBar.foreground": "#e7e7e7",
        "statusBarItem.hoverBackground": "#164a8e",
        "statusBarItem.remoteBackground": "#0f3362",
        "statusBarItem.remoteForeground": "#e7e7e7",
        "titleBar.activeBackground": "#0f3362",
        "titleBar.activeForeground": "#e7e7e7",
        "titleBar.inactiveBackground": "#0f336299",
        "titleBar.inactiveForeground": "#e7e7e799"
    },
    "peacock.color": "#0f3362",
    "todo-tree.tree.scanMode": "workspace",

    "todo-tree.general.tags": [
        "BUG",
        "HACK",
        "FIXME",
        "TODO",
        "FORMULA"
    ],

    "todo-tree.highlights.customHighlight": {
        "BUG": {
            "foreground": "#00FF00",
            "gutterIcon": true
        },
        "HACK": {
            "icon": "zap",
            "foreground": "#fbff00",
            "gutterIcon": true
        },
        "FIXME": {
            "foreground": "#ff0000",
            "gutterIcon": true
        },
        "TODO": {
            "foreground": "#FF8C00"
        },
        "FORMULA": {
            "icon": "graph",
            "foreground": "#ff00ff",
            "gutterIcon": true
        }
    },
}